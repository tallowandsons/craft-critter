{
    "name": "tallowandsons/craft-critter",
    "description": "Automated critical css for Craft CMS",
    "type": "craft-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "Tallow & Sons",
            "homepage": "https://github.com/tallowandsons"
        }
    ],
    "support": {
        "email": "michael@honcho.agency",
        "issues": "https://github.com/tallowandsons/craft-critter/issues?state=open",
        "source": "https://github.com/tallowandsons/craft-critter",
        "docs": "https://github.com/tallowandsons/craft-critter/blob/master/README.md"
    },
    "require": {
        "php": ">=8.2",
        "craftcms/cms": "^5.5.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "tallowandsons\\critter\\": "src/"
        }
    },
    "extra": {
        "handle": "critter",
        "name": "Critter",
        "developer": "Tallow & Sons",
        "developerUrl": "https://github.com/tallowandsons",
        "documentationUrl": "",
        "class": "tallowandsons\\critter\\Critter"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
